    "@types/gapi": "^0.0.43",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/web-push": "^3.3.2",
    "basic-auth": "^2.0.1",
    "bootstrap-icons": "^1.10.3",
    "client-oauth2": "^4.3.3",
    "config": "^3.3.9",
